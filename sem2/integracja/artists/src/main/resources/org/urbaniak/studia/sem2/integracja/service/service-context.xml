<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:ws="http://jax-ws.dev.java.net/spring/core" xmlns:wss="http://jax-ws.dev.java.net/spring/servlet"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
        http://jax-ws.dev.java.net/spring/core
        https://jax-ws.dev.java.net/spring/core.xsd
        http://jax-ws.dev.java.net/spring/servlet
        https://jax-ws.dev.java.net/spring/servlet.xsd">

    <wss:binding url="/music.ws">
        <wss:service>
            <ws:service bean="#musicService" />
        </wss:service>
    </wss:binding>

    <bean id="musicService"
        class="org.urbaniak.studia.sem2.integracja.service.MusicServiceImpl" />
</beans>